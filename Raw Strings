fun main(){ 
    var name:String = 
    """
    Ram
    Ram
    Ram
    Ram
    Ram
    """
    println(name)
    
    var name1:String = 
    """
    Ram
     Ram 
       Ram
         Ram
           Ram
           """
    
    println(name1)
    
    var name2:String = 
    """
    Ram
    	Ram
    		Ram
    			Ram
    				Ram
    """
    println(name2)
    
    var name3:String = 
    """
    	Ram
    Ram		Ram
    	Ram
   
    """
    println(name3)

}
